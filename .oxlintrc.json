{
  "$schema": "./node_modules/oxlint/configuration_schema.json",
  "categories": {
    "correctness": "error",
    "suspicious": "warn",
    "pedantic": "off",
    "perf": "warn",
    "style": "off",
    "restriction": "off",
    "nursery": "off"
  },
  "rules": {
    "no-console": "off",
    "no-debugger": "warn",
    "no-alert": "error",
    "no-unused-vars": "warn",
    "@typescript-eslint/no-unused-vars": "warn",
    "no-undef": "error",
    "eqeqeq": ["error", "always"]
  },
  "plugins": ["typescript", "unicorn", "import", "promise"],
  "ignorePatterns": [
    "node_modules/",
    ".nuxt/",
    ".output/",
    "dist/",
    "coverage/",
    "supabase/",
    ".claude/skills/",
    "*.d.ts"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es2024": true
  },
  "globals": {
    "defineNuxtConfig": "readonly",
    "defineNuxtPlugin": "readonly",
